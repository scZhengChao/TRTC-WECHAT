/* components/agora-player/agora-player.wxss */

@import "../../common.wxss";
.play-container {
  background: #FFF;
  display: block;
  position: absolute;
}

.sud {
  background-color: #fff;
  color: #000;
  position: absolute;
  width: 100%;
  justify-content: center;
  height: 100%;
  text-align: center;
}

.radio-container {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  height: 100%;
  width: 100%;
  background-color: #FFF;
  color: #000
}

.radio-users-double {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center
}

.agentinfo-avatar-wrapper-mini,
.userinfo-avatar-wrapper-mini {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 176rpx;
  width: 176rpx;
  border-radius: 176rpx;
  background-color: #FFF;
  box-shadow: 0 2px 8px 0 rgba(0, 0, 0, 0.3);
}

.agentinfo-avatar-mini,
.userinfo-avatar-mini {
  height: 168rpx;
  width: 168rpx;
  border-radius: 168rpx;
}

.agentinfo-avatar-wrapper {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 252rpx;
  width: 252rpx;
  border-radius: 126rpx;
  background-color: #FFF;
  box-shadow: 0 2px 8px 0 rgba(0, 0, 0, 0.3);
  margin-bottom: 116rpx;
}

.radio-double-img {
  width: 200rpx;
  height: 80rpx;
}

.agentinfo-avatar {
  height: 240rpx;
  width: 240rpx;
  border-radius: 120rpx;
}

.radio-users-single {
  margin-bottom: 200rpx;
}

.icon-fullscreen {
  position: absolute;
  bottom: 10px;
  right: 10px;
  height: 28rpx;
  width: 28rpx;
}

.cover-fullscreen {
  position: absolute;
  /* background-image: url(data:image/png;base64,); */
  border: 1px solid #000;
  left: 0px;
  top: 0px;
  height: 200rpx;
  width: 100%;
}

.cover-fullscreen-background {
  width: 100%;
  height: 100%;
  position: absolute;
}

.cover-fullscreen-back {
  position: absolute;
  margin-top: 60rpx;
  margin-left: 40rpx;
  height: 28rpx;
  width: 20rpx;
}